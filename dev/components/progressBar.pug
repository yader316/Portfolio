mixin ProgressBar(title,img,porcentage)
  .progressbar.col-xs-12.col-sm-6.col-md-4.col-lg-3
    div(class="progressbar__round" data-value=`${porcentage}` data-size="200" data-thickness="12")
      strong.progressbar__circleContainer
      span.progressbar__title #[img(src=`public/img/icon/${img}`, alt="")] #{title}
        
